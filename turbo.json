{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "RAPPEL_CONSO_API_URL",
    "KV_REST_API_URL",
    "KV_REST_API_TOKEN",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_VERCEL_URL",
    "NEXT_PUBLIC_VERCEL_ENV",
    "NEXT_PUBLIC_SIMPLE_ANALYTICS_BASE_URL",
    "NEXT_PUBLIC_SIMPLE_ANALYTICS_SCRIPT_URL",
    "NEXT_PUBLIC_SIMPLE_ANALYTICS_HOSTNAME"
  ],
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
